{
  "name": "n8n-nodes-roam",
  "version": "0.1.3",
  "description": "n8n community node to work with the Roam API",
  "license": "MIT",
  "homepage": "https://ro.am",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Rob Figueiredo",
    "email": "robfig@ro.am"
  },
  "repository": {
